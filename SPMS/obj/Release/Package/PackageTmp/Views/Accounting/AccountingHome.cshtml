@model SPMS.Models.Accounting

@{
    ViewBag.Title = "AccountingHome";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

@using (Html.BeginForm()) 
{
    @Html.AntiForgeryToken()
    
    <div class="form-horizontal">
        <h3>Enter the Student School Id for Accounting Details </h3>

        <hr  />
        @Html.ValidationSummary(true)

        
        
        
        <div class="form-group">
            @Html.LabelFor(model => model.StdSchoolId, new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.EditorFor(model => model.StdSchoolId)
                @Html.ValidationMessageFor(model => model.StdSchoolId)
            </div>
        </div>

        
        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Submit" class="btn btn-default" />
            </div>
        </div>
    </div>
}

<div>
    @Html.ActionLink("Back to List", "Index")
</div>
